{
    "name": "HyRead圖書館註記匯出",
    "description": "在HyRead圖書館服務的裡面增加註記匯出功能",
    "version": "0.3",
    "manifest_version": 3,
    "background": {
        "service_worker": "dist/background-bundle.js"
    },
    "content_scripts": [
        {
            "matches": ["https://*.ebook.hyread.com.tw/*"],
            "all_frames": true,
            "js": [
                "dist/content-script-bundle.js"
            ]
        }
    ]
}
